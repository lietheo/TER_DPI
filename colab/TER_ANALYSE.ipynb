      "source": [
        "!pip install geopandas networkx shapely python-docx\n",
        "\n",
        "import geopandas as gpd\n",
        "import networkx as nx\n",
        "from shapely.geometry import Point, LineString\n",
        "import random\n",
        "import os\n",
        "from docx import Document\n",
        "from zipfile import ZipFile\n",
        "from collections import defaultdict\n",
        "\n",
        "# --- Paramètres ---\n",
        "shapefile_path = \"XXX.shp"  # Linéaire d'un réseau\n",
        "output_folder = \"XXX"\n",
        "os.makedirs(output_folder, exist_ok=True)\n",
        "\n",
        "# --- Charger réseau ---\n",
        "gdf = gpd.read_file(shapefile_path)\n",
        "\n",
        "# --- Construire graphe pondéré ---\n",
        "G = nx.Graph()\n",
        "for idx, row in gdf.iterrows():\n",
        "    geom = row.geometry\n",
        "    if isinstance(geom, LineString):\n",
        "        start = tuple(geom.coords[0])\n",
        "        end = tuple(geom.coords[-1])\n",
        "        length = geom.length\n",
        "        G.add_edge(start, end, weight=length, id=idx)\n",
        "\n",
        "# --- Calcul centralités ---\n",
        "def compute_centralities(graph):\n",
        "    return {\n",
        "        'betweenness': nx.betweenness_centrality(graph, weight='weight'),\n",
        "        'closeness': nx.closeness_centrality(graph, distance='weight'),\n",
        "        'degree': nx.degree_centrality(graph)\n",
        "    }\n",
        "\n",
        "centralities_before = compute_centralities(G)\n",
        "\n",
        "# --- Fonction suppression nœuds ---\n",
        "def simulate_attack(graph, nodes_to_remove):\n",
        "    G_copy = graph.copy()\n",
        "    G_copy.remove_nodes_from(nodes_to_remove)\n",
        "    return G_copy\n",
        "\n",
        "# --- Attaque ciblée ---\n",
        "nodes_sorted = sorted(centralities_before['betweenness'].items(), key=lambda x: x[1], reverse=True)\n",
        "targeted_nodes = [n for n, val in nodes_sorted[:5]]\n",
        "G_targeted = simulate_attack(G, targeted_nodes)\n",
        "centralities_targeted = compute_centralities(G_targeted)\n",
        "\n",
        "# --- Attaque aléatoire : répétitions ---\n",
        "N_REPETITIONS = 25\n",
        "num_remove = 5\n",
        "\n",
        "# Initialisation des accumulateurs de centralités\n",
        "accumulated_centralities = {\n",
        "    'betweenness': defaultdict(float),\n",
        "    'closeness': defaultdict(float),\n",
        "    'degree': defaultdict(float)\n",
        "}\n",
        "\n",
        "for i in range(N_REPETITIONS):\n",
        "    random_nodes = random.sample(list(G.nodes), num_remove)\n",
        "    G_random = simulate_attack(G, random_nodes)\n",
        "    centralities_random = compute_centralities(G_random)\n",
        "    # Accumuler valeurs (mettre 0 si noeud absent)\n",
        "    for key in accumulated_centralities.keys():\n",
        "        for node in G.nodes:\n",
        "            val = centralities_random[key].get(node, 0)\n",
        "            accumulated_centralities[key][node] += val\n",
        "\n",
        "# Moyenne des centralités post attaque aléatoire\n",
        "centralities_random_mean = {\n",
        "    key: {node: accumulated_centralities[key][node]/N_REPETITIONS for node in G.nodes}\n",
        "    for key in accumulated_centralities.keys()\n",
        "}\n",
        "\n",
        "# --- Synthèse rapport ---\n",
        "def summarize(graph_before, graph_after, central_before, central_after, removed_nodes, attack_type):\n",
        "    report = f\"--- Rapport d'attaque {attack_type} ---\\n\"\n",
        "    report += f\"Nombre de nœuds avant : {graph_before.number_of_nodes()}\\n\"\n",
        "    report += f\"Nombre de nœuds après suppression : {graph_after.number_of_nodes()}\\n\"\n",
        "    report += f\"Nœuds supprimés : {removed_nodes}\\n\"\n",
        "    report += \"Changements de centralités moyennes :\\n\"\n",
        "    for key in central_before.keys():\n",
        "        before_mean = sum(central_before[key].values()) / len(central_before[key])\n",
        "        after_mean = sum(central_after[key].values()) / len(central_after[key]) if len(central_after[key]) > 0 else 0\n",
        "        diff = before_mean - after_mean\n",
        "        report += f\"  - {key} : avant={before_mean:.4f}, après={after_mean:.4f}, diff={diff:.4f}\\n\"\n",
        "    return report\n",
        "\n",
        "report_targeted = summarize(G, G_targeted, centralities_before, centralities_targeted, targeted_nodes, \"ciblée\")\n",
        "\n",
        "# Pour rapport aléatoire, on simule un graph moyen avec suppression moyenne\n",
        "nodes_removed_random_report = \"[Liste non précise - moyenne de 25 attaques]\"\n",
        "G_random_report = G.copy()\n",
        "# On enlève arbitrairement num_remove noeuds\n",
        "nodes_to_remove_report = random.sample(list(G.nodes), num_remove)\n",
        "G_random_report.remove_nodes_from(nodes_to_remove_report)\n",
        "report_random = summarize(G, G_random_report, centralities_before, centralities_random_mean, nodes_to_remove_report, \"aléatoire (moyenne 25 rép.)\")\n",
        "\n",
        "print(report_targeted)\n",
        "print(report_random)\n",
        "\n",
        "# --- Export shapefiles ---\n",
        "def export_nodes_centrality(graph, centralities, path):\n",
        "    nodes = list(graph.nodes)\n",
        "    gdf_nodes = gpd.GeoDataFrame({\n",
        "        'betweenness': [centralities['betweenness'].get(n, 0) for n in nodes],\n",
        "        'closeness': [centralities['closeness'].get(n, 0) for n in nodes],\n",
        "        'degree': [centralities['degree'].get(n, 0) for n in nodes],\n",
        "        'geometry': [Point(n) for n in nodes]\n",
        "    }, crs=gdf.crs)\n",
        "    gdf_nodes.to_file(path)\n",
        "\n",
        "export_nodes_centrality(G, centralities_before, os.path.join(output_folder, \"noeuds_avant.shp\"))\n",
        "export_nodes_centrality(G_targeted, centralities_targeted, os.path.join(output_folder, \"noeuds_apres_attaque_cible.shp\"))\n",
        "export_nodes_centrality(G, centralities_random_mean, os.path.join(output_folder, \"noeuds_apres_attaque_aleatoire.shp\"))\n",
        "\n",
        "# --- Création document Word ---\n",
        "doc = Document()\n",
        "doc.add_heading('Rapport d’analyse de vulnérabilité réseau', 0)\n",
        "\n",
        "doc.add_heading('Attaque ciblée (betweenness)', level=1)\n",
        "doc.add_paragraph(report_targeted)\n",
        "\n",
        "doc.add_heading('Attaque aléatoire (moyenne sur 25 répétitions)', level=1)\n",
        "doc.add_paragraph(report_random)\n",
        "\n",
        "word_path = os.path.join(output_folder, \"rapport_vulnerabilite.docx\")\n",
        "doc.save(word_path)\n",
        "\n",
        "# --- Création archive ZIP ---\n",
        "zip_path = os.path.join(output_folder, \"resultats_vulnerabilite.zip\")\n",
        "with ZipFile(zip_path, 'w') as zipf:\n",
        "    # Ajouter tous les fichiers shapefile\n",
        "    for shp_file in [\"noeuds_avant\", \"noeuds_apres_attaque_cible\", \"noeuds_apres_attaque_aleatoire\"]:\n",
        "        for ext in [\".shp\", \".shx\", \".dbf\", \".prj\", \".cpg\"]:\n",
        "            file_path = os.path.join(output_folder, shp_file + ext)\n",
        "            if os.path.exists(file_path):\n",
        "                zipf.write(file_path, arcname=shp_file + ext)\n",
        "    # Ajouter le rapport Word\n",
        "    zipf.write(word_path, arcname=\"rapport_vulnerabilite.docx\")\n",
        "\n",
        "print(f\"Tous les fichiers sont zippés dans : {zip_path}\")\n",
        "\n",
        "from google.colab import files\n",
        "files.download(zip_path)\n"
      ]
    }
